  },
  "dependencies": {
    "omit.js": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@react-native-community/eslint-config": "2.0.0",
    "babel-jest": "^26.6.3",
    "cpx": "^1.5.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.2",
    "react": "^17.0.1",
    "react-native": "^0.64.0",
    "react-test-renderer": "^17.0.1",
    "recursive-copy": "^2.0.11",
    "rimraf": "^3.0.2"
  },
  "files": [
    "src"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": "@react-native-community"
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/app/"
